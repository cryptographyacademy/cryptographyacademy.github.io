    <p class="text-gray-300">Rnds = 9 bound = 30 @ bound for the weight ws = 16 @ word size alpha = 7 @ 8 for word size > 16 beta = 2 @ 3 for word size > 16 zero = ’zero’ f = open(’speck’ + str(Rnds) + ’.ys’, ’w’)</p>

    <p class="text-gray-300">def rotr(x1, v0, alpha): return ’(assert (= ’+v0+’ (bv-rotate-right ’+x1+’ ’+str(alpha)+’)))\\n’</p>

    <p class="text-gray-300">def rotl(x2, x5, x4, beta): return ’(assert (= ’+x4+’ (bv-xor ’+x5+’ (bv-rotate-left ’+x2+’ ’+str(beta)+’))))\\n’</p>

    <p class="text-gray-300">def shift1(v0): return ’(bv-shift-left0 ’+v0+’ 1)’</p>

    <p class="text-gray-300">def add(v0, x0, x3): negv0 = ’(bv-not ’+shift1(v0)+’)’ eq = ’(bv-and (bv-xor ’+negv0+’ ’+shift1(x0)+’) (bv-xor ’+negv0+’ ’+shift1(x3)+’))’ sumabc = ’(bv-xor ’+v0+’ ’+x0+’ ’+ x3+’ ’+shift1(x0)+’)’ return ’(assert (= (bv-and ’ + eq + ’ ’ + sumabc + ’) zero))\\n’</p>

    <p class="text-gray-300">def penalty(v0, x0, x3, p0): return ’(assert (= ’+p0+’ (bv-not (bv-and (bv-xor (bv-not ’+v0+’) ’+x0+’) (bv-xor (bv-not ’+v0+’) ’+x3+’)))))\\n’</p>

    <p class="text-gray-300">def weight(p0): s = [] for i in range(ws-1): s.append(’(bv-concat 0b0000000000 (bv-extract ’+str(i)+’ ’+str(i) +’ ’+p0+’))’)</p>

    <p class="text-gray-300">return s</p>

    <p class="text-gray-300">f.write(’; variables to each round\\n’) f.write(’(define ’+zero+’:: (bitvector ’+str(ws)+’))\\n’) states = [’x’ + str(i) for i in range(2<em>Rnds+2)] for i in range(2</em>Rnds+2): f.write(’(define ’+states[i]+’:: (bitvector ’+str(ws)+’))\\n’)</p>

    <p class="text-gray-300">f.write(’; intermediate variables\\n’) vAfterRt = [’v’ + str(i) for i in range(Rnds)] for i in range(Rnds): f.write(’(define ’+vAfterRt[i]+’:: (bitvector ’+str(ws)+’))\\n’)</p>

    <p class="text-gray-300">f.write(’; penalty variables\\n’) pen = [’p’ + str(i) for i in range(Rnds)] for i in range(Rnds): f.write(’(define ’+pen[i]+’:: (bitvector ’+str(ws)+’))\\n’)</p>

    <p class="text-gray-300">f.write(’; weight of the characteristic\\n’) f.write(’(define weight:: (bitvector 11))\\n’n’) f.write(’(assert (= zero (mk-bv ’+str(ws)+’ 0)))\\n’)</p>

    <p class="text-gray-300">for i in range(Rnds): f.write( rotr(states[2<em>i+1], vAfterRt[i], alpha)) f.write(add(vAfterRt[i], states[2</em>i], states[2<em>i+3])) f.write( rotl(states[2</em>i], states[2<em>i+3], states[2</em>i+2], beta)) f.write(penalty(vAfterRt[i], states[2<em>i], states[2</em>i+3], pen[i]))</p>

    <p class="text-gray-300">@f.write(’; No all?zero characteristic :\\n’) f.write(’(assert (/= (bv-or ’+states[0]+’ ’+ states[1] +’) ’+zero+’))\\n’)</p>

    <p class="text-gray-300">sumW = [] for i in range(Rnds): sumW += weight(pen[i]) f.write(’(assert (= weight (bv-add ’+ ’ ’.join(mmW)+’)))\\n’)</p>

    <p class="text-gray-300">f.write(’; fix the input and output differences \\n’) f.write(’(assert (= x0 (mk-bv 16 ’+str(int(0xA000 ))+’)))\\n’) f.write(’(assert (= x1 (mk-bv 16 ’+str(int(0x8054 ))+’)))\\n’) f.write(’(assert (= x’+str(2<em>Rnds )+’ (mk-bv 16 ’+str(int(0x0542 ))+’)))\\n’) f.write(’(assert (= x’+str(2</em>Rnds+1)+’ (mk-bv 16 ’+str(int(0x0040 ))+’)))\\n’)</p>

    <p class="text-gray-300">f.write(’(assert (bv-le weight (mk-bv 11 ’+ str(bound)+’)))\\n’)</p>

    <p class="text-gray-300">f.write(’(check)\\n’) f.write(’(show-modal)\\n’) f.write(’(export-to-dimacs "test.cnf")’)</p>`;
