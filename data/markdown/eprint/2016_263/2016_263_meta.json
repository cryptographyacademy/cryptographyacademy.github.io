  "eprint": {
    "title": "Efficient Zero-Knowledge Arguments for Arithmetic Circuits in the Discrete Log Setting",
    "authors": [
      {
        "name": "Jonathan Bootle",
        "email": "14@ucl",
        "affiliation": ""
      },
      {
        "name": "Andrea Cerulli",
        "email": "13@ucl",
